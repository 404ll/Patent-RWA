# 私钥 (不要提交到版本控制)
PRIVATE_KEY=your_private_key_here

# Infura项目ID
INFURA_PROJECT_ID=your_infura_project_id

# Etherscan API Key (用于合约验证)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Gas报告
REPORT_GAS=true

# IPFS配置
REACT_APP_IPFS_PROJECT_ID=your_ipfs_project_id
REACT_APP_IPFS_SECRET=your_ipfs_secret

# 合约地址 (部署后填入)
REACT_APP_PATENT_TOKEN_ADDRESS=
REACT_APP_PATENT_VESTING_ADDRESS=
REACT_APP_REVENUE_DISTRIBUTION_ADDRESS=
REACT_APP_PATENT_DAO_ADDRESS=
REACT_APP_PATENT_ORACLE_ADDRESS=

# 网络配置
REACT_APP_CHAIN_ID=1
REACT_APP_RPC_URL=https://mainnet.infura.io/v3/your_infura_project_id